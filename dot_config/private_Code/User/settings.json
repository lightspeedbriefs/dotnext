{
  "dart.devToolsBrowser": "default",
  "dev.containers.dockerComposePath": "podman-compose",
  "dev.containers.dockerPath": "podman",
  "editor.fontFamily": "'Monaspace Neon NF','Cascadia Code PL','JetBrains Mono','Font Awesome 6 Brands'",
  "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss06', 'ss09', 'liga'",
  "editor.semanticTokenColorCustomizations": {
    "[One Dark Pro]": {
      "rules": {
        "parameter": "#D19A66",
        "property": "#9D527C",
        "property:python": "#9D527C",
        "variable.readonly:python": "#D19A66"
      }
    }
  },
  "editor.stickyScroll.enabled": true,
  "editor.stickyScroll.defaultModel": "foldingProviderModel",
  "editor.suggest.preview": true,
  "files.autoSave": "afterDelay",
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "git.mergeEditor": true,
  "gitlens.blame.ignoreWhitespace": true,
  "gitlens.telemetry.enabled": false,
  "redhat.telemetry.enabled": false,
  "security.promptForLocalFileProtocolHandling": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.confirmOnExit": "hasChildProcesses",
  "terminal.integrated.confirmOnKill": "always",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.persistentSessionScrollback": 1000,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shellIntegration.enabled": true,
  "textmarker.enableTelemetry": false,
  "vim.easymotion": true,
  "vim.handleKeys": {
      "<C-p>": false,
      "<C-d>": true,
      "<C-s>": false,
      "<C-z>": false
  },
  "vim.highlightedyank.enable": true,
  "vim.sneak": true,
  "window.commandCenter": true,
  "window.menuBarVisibility": "compact",
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "Catppuccin Frappé",
  "workbench.iconTheme": "catppuccin-frappe",
  "workbench.preferredDarkColorTheme": "Catppuccin Frappé"
}
