[status]
disabled = false
map_symbol = true
format = "[$symbol $common_meaning$signal_name$maybe_int]($style) "

[cmd_duration]
show_notifications = true
{{ if eq .theme "dracula" -}}
style = "bold #f1fa8c"

[aws]
style = "bold #ffb86c"

[character]
error_symbol = "[λ](bold #ff5555)"
success_symbol = "[λ](bold #f8f8f2)"

[directory]
style = "bold #50fa7b"

[git_branch]
style = "bold #ff79c6"

[git_status]
style = "bold #ff5555"

[hostname]
style = "bold #ff5555"

[username]
format = "[$user]($style) on "
style_user = "bold #bd93f9"
{{- end }}
